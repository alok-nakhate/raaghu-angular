{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"storybook": {
			"projectType": "application",
			"root": "stories",
			"sourceRoot": "stories",
			"architect": {
				"build": {
					"options": {
						"tsConfig": "tsconfig.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/libs/rds-elements/src/rds-icon/src/assets/",
								"output": "./assets"
							}
						],
						"styles": [
							"projects/libs/themes/src/styles/quillEditor.scss",
							"projects/libs/themes/src/styles/responsive.scss",
							"projects/host/src/styles.css",
							"projects/libs/themes/src/styles/default.scss",
							"./node_modules/ngx-videogular/fonts/videogular.css",
							"./node_modules/bootstrap/dist/css/bootstrap.min.css"
						],
						"scripts": [
							"./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
							"./node_modules/cookieconsent/build/cookieconsent.min.js",
							"./node_modules/bootstrap/dist/js/bootstrap.min.js"
						]
					}
				}
			}
		},
		"build-storybook": {
			"projectType": "application",
			"root": "stories",
			"sourceRoot": "stories",
			"architect": {
				"build": {
					"options": {
						"tsConfig": "tsconfig.json",
						"styles": [
							"projects/libs/themes/src/styles/quillEditor.scss",
							"projects/libs/themes/src/styles/responsive.scss",
							"projects/host/src/styles.css",
							"projects/libs/themes/src/styles/default.scss",
							"./node_modules/ngx-videogular/fonts/videogular.css"
						],
						"scripts": [
							"./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
							"./node_modules/cookieconsent/build/cookieconsent.min.js"
						]
					}
				}
			}
		},
		"@libs/shared": {
			"projectType": "library",
			"root": "projects/libs/shared",
			"sourceRoot": "projects/libs/shared/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/libs/shared/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/libs/shared/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/libs/shared/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/libs/shared/src/test.ts",
						"tsConfig": "projects/libs/shared/tsconfig.spec.json",
						"karmaConfig": "projects/libs/shared/karma.conf.js"
					}
