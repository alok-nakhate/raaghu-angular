		"rds-components": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "projects/rds-components",
			"sourceRoot": "projects/rds-components/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/rds-components",
						"index": "projects/rds-components/src/index.html",
						"main": "projects/rds-components/src/main.ts",
						"polyfills": "projects/rds-components/src/polyfills.ts",
						"tsConfig": "projects/rds-components/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/rds-components/src/favicon.ico",
							"projects/rds-components/src/assets"
						],
						"styles": [
							"projects/rds-components/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/rds-components/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"optimization": {
							"scripts": true,
							"styles": {
								"minify": true,
								"inlineCritical": true
							},
							"fonts": true
						},
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "15kb",
									"maximumError": "20kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/rds-components/src/environments/environment.ts",
									"with": "projects/rds-components/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/rds-components/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "rds-components:build:production",
							"extraWebpackConfig": "projects/rds-components/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "rds-components:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"open": false,
						"port": 5000,
						"publicHost": "http://localhost:5000",
						"extraWebpackConfig": "projects/rds-components/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "rds-components:build",
						"extraWebpackConfig": "projects/rds-components/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/rds-components/src/test.ts",
						"polyfills": "projects/rds-components/src/polyfills.ts",
						"tsConfig": "projects/rds-components/tsconfig.spec.json",
						"karmaConfig": "projects/rds-components/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/rds-components/src/favicon.ico",
							"projects/rds-components/src/assets"
						],
						"styles": [
							"projects/rds-components/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
